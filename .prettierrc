{
	"trailingComma": "all",
	"tabWidth": 1,
	"semi": true,
	"singleQuote": true,
	"allowParens": "always",
	"useTabs": true,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"arrowParens": "always",
	"proseWrap": "always",
	"quoteProps": "as-needed",
	"singleAttributePerLine": true,
	"printWidth": 80,
	"endOfLine": "auto"
}
